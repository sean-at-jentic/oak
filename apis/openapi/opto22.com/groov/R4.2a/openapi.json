        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "items": {
                    "$ref": "#/components/schemas/tagDefinition"
                  },
                  "type": "array"
                }
              }
            },
            "description": "OK"
          }
        },
        "security": [
          {
            "api_key": []
          }
        ],
        "tags": [
          "data-store"
        ],
        "description": "List tags of the given device. Authorized for admins and editors.",
        "operationId": "listDeviceTags"
      }
    },
    "/v1/data-store/read": {
      "parameters": [],
      "post": {
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "items": {
                    "$ref": "#/components/schemas/tagValue"
                  },
                  "type": "array"
                }
              }
            },
            "description": "OK"
          }
        },
        "security": [
          {
            "api_key": []
          }
