    "/v1/data-store/read/{id}": {
      "parameters": [
        {
          "description": "ID of the tag to read.",
          "in": "path",
          "name": "id",
          "required": true,
          "schema": {
            "format": "int64",
            "type": "number"
          }
        },
        {
          "description": "Table index to start reading at.",
          "in": "query",
          "name": "index",
          "required": false,
          "schema": {
            "default": 0,
            "format": "int32",
            "type": "number"
          }
        },
        {
          "description": "Number of elements to read from a table.",
          "in": "query",
          "name": "count",
          "required": false,
          "schema": {
            "default": 1,
            "format": "int32",
            "type": "number"
          }
        }
      ],
      "get": {
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
