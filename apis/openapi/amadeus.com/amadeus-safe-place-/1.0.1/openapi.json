  "paths": {
    "/safety/safety-rated-locations": {
      "get": {
        "parameters": [
          {
            "description": "Latitude (decimal coordinates)",
            "example": 41.397158,
            "in": "query",
            "name": "latitude",
            "required": true,
            "schema": {
              "format": "double",
              "type": "number"
            }
          },
          {
            "description": "Longitude (decimal coordinates)",
            "example": 2.160873,
            "in": "query",
            "name": "longitude",
            "required": true,
            "schema": {
              "format": "double",
              "type": "number"
            }
          },
          {
            "description": "radius of the search in Kilometer. Can be from 0 to 20, default value is 1 Km.",
            "in": "query",
            "name": "radius",
            "required": false,
            "schema": {
              "default": 1,
              "maximum": 20,
              "minimum": 0,
              "type": "integer"
            }
          },
          {
            "$ref": "#/components/parameters/pageLimit"
          },
          {
            "$ref": "#/components/parameters/pageOffset"
          }
        ],
        "responses": {
          "200": {
            "$ref": "#/components/responses/safety-rated-locations"
          },
          "400": {
            "$ref": "#/components/responses/400"
          },
          "default": {
            "$ref": "#/components/responses/500"
          }
        },
        "tags": [
          "Search"
        ],
        "deprecated": true,
        "description": "",
        "operationId": "getSafetyRanking",
        "summary": "Returns safety rating for a given location and radius."
      }
    },
    "/safety/safety-rated-locations/by-square": {
      "get": {
        "parameters": [
          {
            "description": "Latitude north of bounding box (decimal coordinates)",
            "example": 41.397158,
            "in": "query",
            "name": "north",
            "required": true,
            "schema": {
              "format": "double",
              "type": "number"
            }
          },
          {
            "description": "Longitude west of bounding box (decimal coordinates)",
            "example": 2.160873,
            "in": "query",
            "name": "west",
            "required": true,
            "schema": {
              "format": "double",
              "type": "number"
            }
          },
          {
            "description": "Latitude south of bounding box (decimal coordinates)",
            "example": 41.394582,
            "in": "query",
            "name": "south",
            "required": true,
            "schema": {
              "format": "double",
              "type": "number"
            }
          },
          {
            "description": "Longitude east of bounding box (decimal coordinates)",
            "example": 2.177181,
            "in": "query",
            "name": "east",
            "required": true,
            "schema": {
              "format": "double",
              "type": "number"
            }
          },
          {
            "$ref": "#/components/parameters/pageLimit"
          },
          {
            "$ref": "#/components/parameters/pageOffset"
          }
        ],
        "responses": {
          "200": {
            "$ref": "#/components/responses/safety-rated-locations"
          },
          "400": {
            "$ref": "#/components/responses/400"
          },
          "default": {
            "$ref": "#/components/responses/500"
          }
        },
        "tags": [
          "Search"
        ],
        "deprecated": true,
        "description": "",
        "operationId": "getSafetyRankBySquare",
        "summary": "Returns the safety rating of a given area"
      }
    },
    "/safety/safety-rated-locations/{safety-rated-locationId}": {
      "parameters": [
        {
          "$ref": "#/components/parameters/safety-rated-locationId"
        }
      ],
      "get": {
        "responses": {
          "200": {
            "$ref": "#/components/responses/safety-rated-location"
          },
          "400": {
            "$ref": "#/components/responses/400"
          },
          "404": {
            "$ref": "#/components/responses/404"
          },
          "default": {
            "$ref": "#/components/responses/500"
          }
        },
        "tags": [
          "Retrieve"
        ],
        "deprecated": true,
        "description": "",
        "operationId": "getLocationSafetyRanking",
        "summary": "Retieve safety information of a location by its Id."
      }
    }
